@page
@model checkpoint_web.Pages.Admin.Ubicaciones.DeleteModel
@{
 ViewData["Title"] = "Eliminar Ubicación";
 Layout = null;
}
<h1>Eliminar Ubicación</h1>
<h3>¿Está seguro de que desea eliminar esta ubicación?</h3>
<div>
 <dl class="row">
 <dt class="col-sm-3">Sede</dt>
 <dd class="col-sm-9">@Model.Ubicacion.Sede?.Nombre</dd>
 <dt class="col-sm-3">Código</dt>
 <dd class="col-sm-9">@Model.Ubicacion.Codigo</dd>
 <dt class="col-sm-3">Tipo</dt>
 <dd class="col-sm-9">@Model.Ubicacion.Tipo</dd>
 <dt class="col-sm-3">Capacidad</dt>
 <dd class="col-sm-9">@Model.Ubicacion.Capacidad</dd>
 </dl>
 <form method="post">
 <input type="hidden" asp-for="Ubicacion.Id" />
 <button type="submit" class="btn btn-danger">Eliminar</button>
 <a class="btn btn-secondary" asp-page="Index">Cancelar</a>
 </form>
</div>
